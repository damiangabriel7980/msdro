<!doctype html>
<html lang="en" ng-app="publicApp">
<head>
    <meta charset="UTF-8">
    <title>MSD Be Well</title>
    <!--Get amazon bucket-->
    <script>
        sessionStorage.defaultAmazonBucket = "<%=amazonBucket%>";
    </script>

    <!--Core-modules-->
    <script src="components/angular/angular.js"></script>
    <script src="components/angular-sanitize/angular-sanitize.js"></script>
    <script src="components/angular-resource/angular-resource.js"></script>
    <script src="components/angular-route/angular-route.js"></script>
    <script src="components/angular-ui-router/release/angular-ui-router.js"></script>
    <script src="components/angular-bootstrap/ui-bootstrap-tpls.js"></script>


    <!--Controllers-->
    <script src="javascript/public/app.js"></script>
    <script src="javascript/public/publicControllers.js"></script>
    <script src="javascript/public/publicServices.js"></script>
    <script src="javascript/public/publicDirectives.js"></script>
    <script src="javascript/public/home/HomeController.js"></script>
    <script src="javascript/public/home/NoutatiController.js"></script>
    <script src="javascript/public/home/CeleMaiCititeController.js"></script>
    <script src="javascript/public/home/HomeDownloadsController.js"></script>
    <script src="javascript/public/footerController.js"></script>
    <script src="javascript/public/modals/contactModalController.js"></script>


    <!--Plug-ins-->
    <script src="components/shifty/dist/shifty.min.js"></script>
    <script src="components/angular-touch/angular-touch.min.js"></script>
    <script src="components/angular-carousel/dist/angular-carousel.min.js"></script>
    <script src="components/socket.io-client/socket.io.js"></script>
    <script src="modules/custom_timeline/timelineDirective.js"></script>
    <link rel="stylesheet" href="modules/custom_timeline/timelineStyles.css">


    <!--Stylesheets-->
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="components/angular-carousel/dist/angular-carousel.min.css">
    <link rel="stylesheet" href="stylesheets/public/defaultStyles.css">
</head>
<body>

<div id="publicHeaderWrapper">
    <div id="publicHeader">
        <!-- Fixed navbar -->
        <div class="navbar navbar-default navbar-fixed-top ng-scope" role="navigation">
            <div class="container">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" ng-init="navCollapsed = true" ng-click="navCollapsed = !navCollapsed">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <div class="headerSearchAndLinks">
                        <div class="item">
                            <ul>
                                <li>
                                    <a href="#">Manualul Merck</a>
                                </li>
                                <li>|</li>
                                <li>
                                    <a href="#">Contact</a>
                                </li>
                                <li>|</li>
                                <li>
                                    <a href="/login">Log in</a>
                                </li>
                            </ul>
                        </div>
                        <div class="item">
                            <form class="navbar-form" role="search">
                                <div class="input-group">
                                    <input type="text" class="form-control" placeholder="Search">
                                    <span class="input-group-addon"><i class="glyphicon glyphicon-search"></i></span>
                                </div>
                                <button type="submit" class="btn btn-default sr-only">Submit</button>
                            </form>
                        </div>
                    </div>
                    <a class="navbar-brand" href="#"><img ng-src="{{pathAmazonResources+'msd-logo.png'}}"></a>
                </div>

                <div class="collapse navbar-collapse" collapse="navCollapsed">

                    <ul class="nav navbar-nav">
                        <li ng-class="{'active': $state.includes('home')}"><a ui-sref="home.noutati" ng-click="navCollapsed = true"> Home </a></li>
                        <li ng-class="{'active': $state.includes('stiri')}"><a ui-sref="stiri.all" ng-click="navCollapsed = true"> Stiri </a></li>
                        <li ng-class="{'active': $state.includes('articole')}"><a ui-sref="articole.all" ng-click="navCollapsed = true"> Articole </a></li>
                        <li ng-class="{'active': $state.includes('elearning')}"><a ui-sref="elearning.all" ng-click="navCollapsed = true"> Elearning </a></li>
                        <li ng-class="{'active': $state.includes('downloads')}"><a ui-sref="downloads.all" ng-click="navCollapsed = true"> Downloads </a></li>
                    </ul>
                    <ul class="nav navbar-nav navbar-right searchAndLinks">
                        <li>
                            <form class="navbar-form navbar-right" role="search">
                                <div class="input-group">
                                    <input type="text" class="form-control" placeholder="Search">
                                    <span class="input-group-addon"><i class="glyphicon glyphicon-search"></i></span>
                                </div>
                                <button type="submit" class="btn btn-default sr-only">Submit</button>
                            </form>
                        </li>
                        <li class="navbar-link">
                            <a href="#">Manualul Merck</a>
                        </li>
                        <li class="link-divider"></li>
                        <li class="navbar-link">
                            <a href="#">Contact</a>
                        </li>
                        <li class="link-divider"></li>
                        <li class="navbar-link">
                            <a href="/login">Log in</a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</div>

<div id="publicContentWrapper">
    <div id="publicContent">
        <div ui-view class="view-container rootView"></div>
    </div>
</div>

<div class="col-md-12 footer text-center">
    <hr>
    <a href="#">Home</a>&nbsp;|&nbsp;
    <a href="http://www.merckmanuals.com" target="_blank">Manualul Merck</a>&nbsp;|&nbsp;
    <a style="cursor: pointer" ng-controller="FooterController" ng-click="showContactModal()">Contact</a>
</div>

</body>
</html>