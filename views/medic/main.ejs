    <script src="components/angular-growl/build/angular-growl.js"></script>
    <script src="modules/my_slider/slider.js"></script>
    <script src="components/angular-timer/app/js/timer.js"></script>

<!--Stylesheets-->

    <link rel='stylesheet' href='components/angular-ui-select/dist/select.min.css' />
    <link rel='stylesheet' href='components/select2/select2.css' />
    <link rel="stylesheet" href="components/bootstrap/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="stylesheets/style.css">
    <link rel="stylesheet" href="stylesheets/customStyles.css">
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css">
</head>
<body>

<!-- Fixed navbar -->
<div class="navbar navbar-default navbar-fixed-top ng-scope" role="navigation" ng-controller="SpecialGroupsMenuController">
    <div class="container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" ng-init="navCollapsed = true" ng-click="navCollapsed = !navCollapsed">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="#"><img ng-src="{{MSDlogo}}"></a>

            <form class="hidden-md hidden-lg hiddenSearch" role="search">
                <div class="input-group">
                    <input type="text" class="form-control" placeholder="Search" name="q">
                    <div class="input-group-btn">
                        <button class="btn btn-default" type="submit"><i class="glyphicon glyphicon-search"></i></button>
                    </div>
                </div>
            </form>
            <ul class="hidden-md hidden-lg navbar-brand pull-right" style="list-style: none;">
                <li class="dropdown">
                    <a href="#" class="dropdown-toggle profileClass" data-toggle="dropdown">
                        <span class="glyphicon glyphicon-user"></span>
                    </a>
                    <ul class="dropdown-menu profile-drop" role="menu">
                        <li><a style="cursor: pointer;" ng-click="showProfile()">Profile</a></li>
                        <li><a href="logout">Sign Out</a></li>
                    </ul>
                </li>
            </ul>
        </div>

        <div class="collapse navbar-collapse" collapse="navCollapsed">

            <ul class="nav navbar-nav">
                <li ng-class="{'active': $state.includes('home')}"><a ui-sref="home" href="#/"><span class="glyphicon glyphicon-home"></span></a></li>
                <li class="dropdown" ng-class="{'active': $state.includes('noutati')}">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                        <span class="glyphicon glyphicon-globe"></span> Noutati <b class="caret"></b>
                    </a>
                    <ul class="dropdown-menu" role="menu">
                        <li><a ui-sref="noutati.listaArticole({articleType:1})">Stiri</a></li>
                        <li class="divider"></li>
                        <li><a ui-sref="noutati.listaArticole({articleType:2})">Stiri legislative</a></li>
                    </ul>
                </li>
                <li class="dropdown" ng-class="{'active': $state.includes('biblioteca')}">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                        <span class="glyphicon glyphicon-book"></span> Biblioteca <b class="caret"></b>
                    </a>
                    <ul class="dropdown-menu" role="menu">
                        <li><a ui-sref="biblioteca.produse.productsByArea({id:0})" href="#/produse">Produse</a></li>
                        <li class="divider"></li>
                        <li><a ui-sref="biblioteca.articoleStiintifice.listaArticole({articleType:3})">Articole stiintifice</a></li>
                        <li class="divider"></li>
                        <li><a ui-sref="biblioteca.manualMerck" href="http://www.merckmanuals.com/" target="_blank">Manualul Merck</a></li>
                    </ul>
                </li>
                <li ng-class="{'active': $state.includes('calendar')}"><a ui-sref="calendar" href="#/calendar"><span class="glyphicon glyphicon-calendar">Calendar</span></a></li>
                <li class="dropdown" ng-class="{'active': $state.includes('elearning')}">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                        <span class="glyphicon glyphicon-download"></span> E-learning <b class="caret"></b>
                    </a>
                    <ul class="dropdown-menu" role="menu">
                        <li><a ui-sref="elearning.multimedia.multimediaByArea({id:0})" href="#/multimedia">Multimedia</a></li>
                        <li class="divider"></li>
                        <li><a ui-sref="elearning.transmisii" href="#/transmisii">Transmisii live</a></li>
                        <li class="divider"></li>
                        <li><a ui-sref="elearning.teste" href="#/teste">Teste interactive</a></li>
                    </ul>
                </li>
                <li class="dropdown" ng-class="{'active': $state.includes('groupFeatures')}" ng-if="groupFeatures">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                        <span class="glyphicon glyphicon-download"></span> {{specialGroupSelected?specialGroupSelected.display_name:""}} <b class="caret"></b>
                    </a>
                    <ul class="dropdown-menu" role="menu">
                        <li ng-repeat="feat in groupFeatures"><a ui-sref="groupFeatures({feature: feat})">{{feat}}</a></li>
                    </ul>
                </li>
            </ul>
            <ul class="nav navbar-nav navbar-right" ng-if="specialGroups.length != 0">
                <li class="dropdown">
                    <a href="#" class="dropdown-toggle specialGroupDropdown" data-toggle="dropdown">
                        <img ng-src="{{specialGroupSelected.image_path?pathAmazonDev+specialGroupSelected.image_path:MSDlogo}}">
                    </a>
                    <ul class="dropdown-menu" role="menu">
                        <li ng-repeat="group in specialGroups">
                            <a style="cursor: pointer;" ng-click="selectSpecialGroup(group)">{{group.display_name}}</a>
                        </li>
                    </ul>
                </li>
            </ul>

            <!--<li>-->
            <!--<form class="navbar-form navbar-right" role="search">-->
            <!--<div class="form-group">-->
            <!--<input type="text" class="form-control" placeholder="Search">-->
            <!--</div>-->
            <!--<button type="submit" class="btn btn-default sr-only">Submit</button>-->
            <!--</form>-->
            <!--</li>-->
        </div>
    </div>
</div>


<div ui-view class="view-container" style="padding-bottom: 50px;"></div>

<div id="footer">
    <div class="col-xs-6">
        <a href="http://www.msd.ro/">
            <img src="https://s3-eu-west-1.amazonaws.com/msdapp/resources/msd_white.png" class="pull-left img-responsive" style="height:28px;margin-right:10px;">
        </a>

    </div>
    <div id="notMsieDiv" class="col-xs-6 text-right">

        <a ui-sref="contact" class="link-footer">Contact </a>
        <a ng-click="showFarmaModal()" class="link-footer">Farmacovigilenta</a>
        <a ng-click="showTermsModal()" class="link-footer">Termeni si conditii</a>
    </div>
</div>

</body>
</html>