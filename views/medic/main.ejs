                        {{specialGroupSelected?specialGroupSelected.display_name:""}} <b class="caret"></b>
                    </a>
                    <ul class="dropdown-menu myMenu greyBackground" role="menu">
                        <li ng-repeat="feat in groupFeatures"><a ui-sref="groupFeatures({feature: feat})" ng-click="navCollapsed = true">{{feat}}</a></li>
