<!doctype html>
<html lang="en" ng-app="streamAdminApp">
<head>
    <meta charset="UTF-8">
    <title>Document</title>
    <!--Get amazon bucket-->
    <script>
        const DEFAULT_AMAZON_BUCKET = "<%=amazonBucket%>";
        const DEFAULT_AMAZON_PREFIX = "<%=amazonPrefix%>";
    </script>

    <!--Core-modules-->
    <script src="components/jquery/dist/jquery.js"></script>
    <script src="components/jquery-ui/jquery-ui.js"></script>
    <script type="text/javascript" src="components/moment/moment.js"></script>
<script type="text/javascript" src="components/bootstrap/dist/js/bootstrap.js"></script>
    <script src="components/angular/angular.js"></script>
    <script type="text/javascript" src="components/angular-bootstrap-datetimepicker/src/js/datetimepicker.js"></script>
    <script src="components/angular-cookies/angular-cookies.js"></script>
    <script src="components/angular-sanitize/angular-sanitize.js"></script>
    <script src="components/angular-resource/angular-resource.js"></script>
    <script src="components/angular-route/angular-route.js"></script>
    <script src="components/angular-ui-router/release/angular-ui-router.js"></script>
    <script src="components/angular-bootstrap/ui-bootstrap-tpls.js"></script>
    <script src="components/aws-sdk/dist/aws-sdk.min.js"></script>
    <script src="components/ng-file-upload/ng-file-upload.js"></script>
    <script src="components/ng-table/dist/ng-table.min.js"></script>

    <!--Controllers-->
    <script src="javascript/streamAdmin/app.js"></script>
    <script src="javascript/streamAdmin/controllers.js"></script>
    <script src="javascript/streamAdmin/services.js"></script>
    <script src="javascript/streamAdmin/adminConference/adminConference.controller.js"></script>
    <script src="javascript/streamAdmin/editConference/editConference.controller.js"></script>
    <script src="javascript/streamAdmin/editSpeakers/editSpeakers.controller.js"></script>
    <script src="javascript/streamAdmin/editViewers/editViewers.controller.js"></script>

    <link rel="stylesheet" href="components/bootstrap/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="stylesheets/stylesThatReignSupreme.css">
    <link rel="stylesheet" href="stylesheets/streamadmin/streamStyles.css">

</head>
<body>

<!-- Fixed navbar -->
<div class="navbar navbar-default navbar-fixed-top ng-scope" role="navigation">
    <div class="container">
        <div class="navbar-header">
            <a class="navbar-toggle" ng-init="navCollapsed = true" ng-click="navCollapsed = !navCollapsed">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </a>
            <a class="navbar-brand" href="#">MSD_Conferences</a>
        </div>

        <div class="collapse navbar-collapse" collapse="navCollapsed">

            <ul class="nav navbar-nav">
                <li ng-class="{'active': $state.includes('transmisiiLive')}"><a ui-sref="transmisiiLive"> Transmisii live </a></li>
            </ul>
            <ul class="nav navbar-nav navbar-right">
                <li class="dropdown">
                    <a class="dropdown-toggle" data-toggle="dropdown">
                        <span class="glyphicon glyphicon-user"></span> <%= user.username %> <b class="caret"></b>
                    </a>
                    <ul class="dropdown-menu" role="menu">
                        <li><a style="cursor: pointer;" ng-click="showProfile()">Profile</a></li>
                        <li class="divider"></li>
                        <li><a href="logout">Sign Out</a></li>
                    </ul>
                </li>
            </ul>
        </div>
    </div>
</div>

<div ui-view class="view-container main-view-container"></div>

</body>
</html>