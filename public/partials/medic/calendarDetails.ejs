<div>
        <div class="modal-header">
            <h3 class="modal-title">{{itemsEvent['name']}}</h3>
        </div>
        <div class="modal-body">
            <div class="row">
                <div class="col-md-8"><p class="medium-lineheight"><strong>Locatie: </strong><strong convert-special conver="itemsEvent.place" ng-bind-html="trustAsHtml(itemsEvent.place)"></strong></p></div>
                <div class="col-md-4"><span class="label label-info pull-right"><i class="icon-facetime-video icon-white"></i>{{itemsEvent.start | asDate | date: 'dd/MM/yyyy'}} - {{itemsEvent.end | asDate | date: 'dd/MM/yyyy'}}</span></div>
                <div class="col-xs-12" convert-special conver="itemsEvent.description" ng-bind-html="trustAsHtml(itemsEvent.description)"></div>
            </div>

        </div>
        <div class="modal-footer">
            <button class="btn btn-warning float-left" ng-click="cancell()">Inchide</button>
        </div>
</div>