<!doctype html>
<head>
    <script src="components/fullcalendar/fullcalendar.js"></script>
    <script src="components/fullcalendar/gcal.js"></script>
    <script src="components/perfect-tooltip/js/jquery.tooltip.js"></script>
    <script src="components/qtip2/jquery.qtip.min.js"></script>
    <link rel='stylesheet' href='components/fullcalendar/fullcalendar.css' />
    <link href='components/qtip2/jquery.qtip.css' rel='stylesheet' />
    <link rel='stylesheet' href='components/perfect-tooltip/css/tooltip.css' />

</head>
<body>
<div class="container" id="content">
    <div class="row-margintop20px">
        <div class="col-xs-3 hidden-xs">
            <h4 class="bottomBorder">EVENIMENTE URMATOARE:</h4>
             <div class="event blue" ng-repeat="eventim in events2Filtered">
                    <div class="row">
                        <div class="col-xs-3 date2">
                            <h2 class="MSDgreen">{{eventim.start | filter: asDate | date:'dd'}}</h2>
                            <h5 class="MSDgreen">{{eventim.start | filter: asDate | date:'MMM'}}</h5>
                          </div>

                        <div class="col-xs-8">
                            <h4 ui-sref="calendar.calendarDetails({id:eventim._id})">{{eventim['name']}}</h4>
                        </div>
                    </div>
                </div>
         </div>
<div id="calendar" calendar="myCalendar" ui-calendar="uiConfig.calendar" ng-model="realEvents" class="well col-xs-8 well-content col-xs-offset-1"></div>
        </div>
</div>
<ng-view></ng-view>
</body>