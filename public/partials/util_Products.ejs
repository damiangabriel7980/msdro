<div class="jumbotron">

    <div class="form-horizontal">
        <div>Add content:</div>
        <div class="form-group">
            <span class="col-xs-2 control-label">Title</span>
            <div class="col-xs-8">
                <div class="form-group row">
                    <div class="col-xs-2">
                        <input type="text" class="form-control" placeholder="Version" id="version_prod" ng-model="newProduct.version_prod">
                    </div>
                    <div class="col-xs-2">
                        <input type="text" class="form-control" placeholder="Description" id="description" ng-model="newProduct.description">
                    </div>
                    <div class="col-xs-2">
                        <input type="text" class="form-control" placeholder="Enable(0/1)" id="enableP" ng-model="newProduct.enableP">
                    </div>
                    <div class="col-xs-2">
                        <input type="text" class="form-control" placeholder="File_path" id="filepath" ng-model="newProduct.file_path">
                    </div>
                    <div class="col-xs-2">
                        <input type="text" class="form-control" placeholder="Image_path" id="imagepath" ng-model="newProduct.image_path">
                    </div>
                    <div class="col-xs-2">
                        <input type="text" class="form-control" placeholder="Name_Product" id="nameprod" ng-model="newProduct.nameP">
                    </div>
              </div>
            </div>
        </div>
        <div class="form-group">
            <a href="javascript:;" class="btn btn-default col-xs-offset-2" ng-click="addProduct()" type="button">Add!</a>
        </div>
    </div>
    <table class="table">
        <tbody ng-repeat="cont in products">
        <tr>
            <td>{{cont.version_prod}}</td>
            <td>{{cont.description}}</td>
            <td>{{cont.enableP}}</td>
            <td>{{cont.file_path}}</td>
            <td>{{cont.image_path}}</td>
            <td>{{cont.last_updated}}</td>
            <td>{{cont.nameP}}</td>
            <td><a href="javascript:;" class="btn btn-danger" ng-click="deleteProduct(cont._id)">Delete</a></td>
        </tr>
        </tbody>
    </table>
</div>